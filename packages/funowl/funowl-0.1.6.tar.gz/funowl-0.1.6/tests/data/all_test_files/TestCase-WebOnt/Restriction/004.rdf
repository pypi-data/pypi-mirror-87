        <owl:Restriction>
           <owl:onProperty rdf:resource="#dp"/>
           <owl:someValuesFrom rdf:resource=
     "http://www.w3.org/2001/XMLSchema#byte" />
        </owl:Restriction>
     </owl:intersectionOf>
   </owl:Class>
   <owl:Class rdf:ID="D">
     <owl:intersectionOf rdf:parseType="Collection">
        <owl:Class rdf:ID="superD"/>
        <owl:Restriction>
           <owl:onProperty rdf:resource="#dp"/>
           <owl:someValuesFrom rdf:resource=
     "http://www.w3.org/2001/XMLSchema#byte" />
        </owl:Restriction>
     </owl:intersectionOf>
   </owl:Class>

</rdf:RDF>