    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xml:base="http://www.w3.org/2002/03owlt/Restriction/consistent004" >
   <owl:Ontology/>
   <owl:DatatypeProperty rdf:ID="dp"/>
   <owl:Class rdf:ID="C">
