<?xml version="1.0"?>
<rdf:RDF
  xml:base  = "http://example.org/"
  xmlns     = "http://example.org/"
  xmlns:owl = "http://www.w3.org/2002/07/owl#"
  xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#">

<owl:Ontology/>

<owl:DatatypeProperty rdf:about="hasSSN" />

<rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
  <owl:hasKey rdf:parseType="Collection">
    <rdf:Description rdf:about="hasSSN" />
  </owl:hasKey>
</rdf:Description>

<rdf:Description rdf:about="Peter">
  <hasSSN>123-45-6789</hasSSN>
</rdf:Description>

<rdf:Description rdf:about="Peter_Griffin">
  <hasSSN>123-45-6789</hasSSN>
</rdf:Description>

<rdf:Description rdf:about="Peter">
  <owl:differentFrom rdf:resource="Peter_Griffin" />
</rdf:Description>

</rdf:RDF>