  {% if report_description.measurement is defined and report_description.measurement is not none %}
  <{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:{{ report_description.measurement.item_name }} xmlns:scale="http://docs.oasis-open.org/ns/emix/2011/06/siscale" xmlns:power="http://docs.oasis-open.org/ns/emix/2011/06/power" >
    <{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:itemDescription>{{ report_description.measurement.item_description }}</{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:itemDescription>
    <{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:itemUnits>{{ report_description.measurement.item_units }}</{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:itemUnits>
    <scale:siScaleCode>{{ report_description.measurement.si_scale_code }}</scale:siScaleCode>
    {% if report_description.measurement.power_attributes %}
    <power:powerAttributes>
      <power:hertz>{{ report_description.measurement.power_attributes.hertz }}</power:hertz>
      <power:voltage>{{ report_description.measurement.power_attributes.voltage }}</power:voltage>
      <power:ac>{{ report_description.measurement.power_attributes.ac|booleanformat }}</power:ac>
    </power:powerAttributes>
    {% endif %}
  </{% if report_description.measurement.item_name == 'customUnit' %}oadr{% else %}power{% endif %}:{{ report_description.measurement.item_name }}>
  {% endif %}